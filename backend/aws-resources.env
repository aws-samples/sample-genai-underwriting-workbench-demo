# AWS Environment Variables for Local Lambda Development
# Updated with actual deployed AWS resources from CloudFormation stack

# DynamoDB Table Names (updated to use separate tables)
set -gx ANALYSIS_TABLE_NAME underwriting-ai-v2-analysis-jobs
set -gx SUBMISSIONS_TABLE_NAME underwriting-ai-v2-submissions

# S3 Bucket Names (from CloudFormation outputs)
# DOCUMENT_BUCKET_NAME=uw-ai-v2-543999415209-landing
set -gx DOCUMENT_BUCKET_NAME uw-ai-v2-543999415209-landing
set -gx MOCK_OUTPUT_BUCKET_NAME uw-ai-v2-543999415209-mock-output

# Bedrock Model ID (keep as is)
set -gx BEDROCK_CHAT_MODEL_ID us.anthropic.claude-3-7-sonnet-20250219-v1:0

# Step Functions State Machine ARN (from CloudFormation outputs)
set -gx STATE_MACHINE_ARN arn:aws:states:us-east-1:543999415209:stateMachine:underwriting-ai-v2-workflow

# AWS Region (update if different)
set -gx AWS_DEFAULT_REGION us-east-1
set -gx AWS_REGION us-east-1 